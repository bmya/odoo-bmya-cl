<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model='ir.config_parameter' id='docson_url'>
            <field name='key'>docsonline.url</field>
            <field name='value'>https://www.documentosonline.cl</field>
        </record>
        <record model='ir.config_parameter' id='docson_tok'>
            <field name='key'>docsonline.token</field>
            <field name='value'>PUT_HERE_YOUR_TOKEN</field>
        </record>
        <record model='ir.config_parameter' id='docson_repln'>
            <field name='key'>docsonline.replace_name</field>
            <field name='value'>True</field>
        </record>
        <record model='ir.config_parameter' id='docson_repli'>
            <field name='key'>docsonline.replace_image</field>
            <field name='value'>True</field>
        </record>
        <record model='ir.config_parameter' id='docson_repls'>
            <field name='key'>docsonline.replace_street</field>
            <field name='value'>False</field>
        </record>
        <record model='ir.config_parameter' id='docson_reple'>
            <field name='key'>docsonline.replace_email</field>
            <field name='value'>False</field>
        </record>
    </data>
</odoo>
